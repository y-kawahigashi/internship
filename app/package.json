{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tsc": "tsc --noEmit",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:unit": "jest --testMatch=\"**/test/unit/**/*.spec.ts\"",
    "test:integration": "jest --testMatch=\"**/test/integration/**/*.test.ts\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:studio": "./node_modules/.bin/dotenv -e .env.local -- prisma studio",
    "prisma:reset": "npm run prisma:migrate:reset && npm run prisma:migrate:dev && npm run prisma:generate && npm run seed",
    "prisma:reset:force": "npm run prisma:migrate:reset -- --force && npm run prisma:migrate:dev && npm run prisma:generate && npm run seed",
    "seed": "tsx src/server/seeds/index.ts"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@date-fns/tz": "^1.4.1",
    "@prisma/client": "^6.19.0",
    "date-fns": "^4.1.0",
    "next": "^16.0.8",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tsx": "^4.21.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "typescript": "5.9.3"
  }
}
